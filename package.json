{
  "name": "vue3-jag-location-picker",
  "version": "1.1.3",
  "description": "A Vue 3 location picker component supporting both Google Maps and AMap (高德地图)",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./style.css": "./dist/vue3-jag-location-picker.css"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "vue",
    "vue3",
    "location-picker",
    "google-maps",
    "amap",
    "高德地图",
    "geolocation",
    "map",
    "coordinates",
    "address"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/vue3-jag-location-picker.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/vue3-jag-location-picker/issues"
  },
  "homepage": "https://github.com/yourusername/vue3-jag-location-picker#readme",
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "dts-bundle-generator": "^2.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitepress": "^1.6.3",
    "vue": "^3.0.0"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "dev": "vite",
    "prepublishOnly": "npm run build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  }
}
